SFC A1L
    VAR t : int; s : int; END_VAR
    
    VAR_INPUT i : int; END_VAR
	
	VAR_OUTPUT o :int; END_VAR
    
    STEP A
        ON active ACTION 
            t := i;
        END_ACTION
    END_STEP


    STEP B
        ON active ACTION 
            s := s + 1;
        END_ACTION
    END_STEP
    
    GOTO true :: A -> B
    GOTO s*s > t :: B -> C
	GOTO s*s <= t :: B -> B
    
    STEP C 
        ON active ACTION 
            o := s;
        END_ACTION 
    END_STEP

END_SFC
