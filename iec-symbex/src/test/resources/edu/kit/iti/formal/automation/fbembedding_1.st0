PROGRAM main
    VAR
        a$j : INT := INT#0;
        a$p : INT := INT#0;
        a$sum : INT := INT#0;
    END_VAR

    VAR_INPUT
        i : INT := INT#0;
    END_VAR

    VAR_OUTPUT
        o : INT := INT#0;
    END_VAR


    a$j := i;
    a$sum := (a$p + a$sum);
    a$p := (a$sum / a$p);
    o := a$p;
END_PROGRAM
