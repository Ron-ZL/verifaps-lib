PROGRAM main

VAR_INPUT  i : INT; END_VAR
VAR_OUTPUT  o : INT; END_VAR
VAR  a$j : INT; END_VAR
VAR  a$p : INT; END_VAR
VAR  a$sum : INT; END_VAR
a$j := i;
(* *******************************************************************************
 *Call of a:a   *****************************************************************
 ******************************************************************************* *)
a$sum := (a$p + a$sum);
a$p := (a$sum / a$p);
(* *******************************************************************************
 *End of call   *****************************************************************
 ******************************************************************************* *)
o := a$p;
END_PROGRAM
