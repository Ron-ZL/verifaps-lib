<?xml version="1.0" encoding="UTF-8"?>
<test-table xmlns="http://formal.iti.kit.edu/exteta-1.1" version="1.1">
    <variables>
        <variable data-type="INT" io="input" name="var_A"/>
        <variable data-type="BOOLEAN" io="input" name="var_B"/>
        <variable data-type="INT" io="output" name="var_C"/>
        <variable data-type="BOOLEAN" io="output" name="var_D"/>
        <constraint data-type="BOOLEAN" name="var_p"/>
        <constraint constraint="42" data-type="INT" name="var_q"/>
        <constraint constraint="-3" data-type="INT" name="var_r"/>
    </variables>
    <steps>
        <step duration="1">
            <cell>=var_q - 3</cell>
            <cell>-</cell>
            <cell>-</cell>
            <cell>=TRUE</cell>
        </step>
        <step duration="1">
            <cell>var_C[-1]</cell>
            <cell>NOT var_p</cell>
            <cell>52</cell>
            <cell>-</cell>
        </step>
    </steps>
</test-table>