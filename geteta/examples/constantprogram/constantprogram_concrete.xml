<?xml version="1.0" encoding="UTF-8"?>
<test-table xmlns="http://formal.iti.kit.edu/exteta-1.1"
            name="Ta"
            version="1.1">

    <variables>
        <variable data-type="INT" io="input" name="i"/>
        <variable data-type="INT" io="output" name="o"/>
        <variable data-type="INT" io="output" name="c"/>
    </variables>

    <steps>
        <step duration="[10,-]">
            <cell>-</cell>
            <cell>i</cell>
            <cell>42</cell>
        </step>
    </steps>

    <options>
        <option key="mode" value="CONCRETE_TABLE"/>
        <option key="cycles.1" value="100"/>
    </options>
</test-table>
