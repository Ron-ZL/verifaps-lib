table A {
    var input I : INT;
    var input O : INT;

    group gA {
        row a { \goto: \fail(B,a); I: 2; O:3 }
        row b { \goto: \miss(C,a); I: <0; O: >0}
    }
}

table B {
    var input I : INT;
    var input O : INT;

    group gB {
        row a { I: 5; O:=2*I }
    }
}

table C {
    var input I : INT;
    var input O : INT;

    group gC {
        row a { I: 6; O:=3*I }
    }
}