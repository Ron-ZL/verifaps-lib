<test-table xmlns="http://formal.iti.kit.edu/exteta-1.1" version="1.1">
    <variables>
        <variable io="input" name="i" data-type="BOOLEAN"/>
        <variable io="output" name="o" data-type="BOOLEAN"/>
    </variables>
    <steps>
        <step duration="[1,1]"><cell>T</cell><cell>F</cell></step>
        <step duration="[1,-]"><cell>T</cell><cell>F</cell></step>
        <step duration="[0,-]"><cell>T</cell><cell>F</cell></step>
        <step duration="omega"><cell>T</cell><cell>F</cell></step>
    </steps>
</test-table>